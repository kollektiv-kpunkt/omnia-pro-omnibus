{
    "name": "omnia-pro-omnibus",
    "version": "1.0.0",
    "description": "Webpage for the cantonal election campaign of the young socialists in Zurich juso-wahlen.ch.",
    "private": true,
    "devDependencies": {
        "cross-env": "^7.0.3",
        "fast-glob": "^3.2.11",
        "go-webp": "^1.3.1",
        "laravel-mix": "^6.0.43",
        "normalize.css": "^8.0.1",
        "postcss-nested": "^5.0.6",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.49.9",
        "sass-loader": "^12.6.0",
        "tailwindcss": "^3.0.23",
        "webpack-cli": "^4.9.2",
        "webpack-livereload-plugin": "^3.0.2"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "find . -name '*:Zone.Identifier' -type f -delete && mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "find . -name '*:Zone.Identifier' -type f -delete && npm run production",
        "production": "mix --production",
        "webp": "cd public && go-webp change=false remove=false"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kollektiv-kpunkt/omnia-pro-omnibus.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kollektiv-kpunkt/omnia-pro-omnibus/issues"
    },
    "homepage": "https://github.com/kollektiv-kpunkt/omnia-pro-omnibus#readme",
    "dependencies": {
        "@swup/overlay-theme": "^1.1.1",
        "@swup/preload-plugin": "^1.0.5",
        "axios": "^1.2.1",
        "feather-icons": "^4.29.0",
        "notyf": "^3.10.0",
        "punycode": "^2.1.1",
        "simple-icons": "^7.21.0",
        "swup": "^2.0.19",
        "vanilla-cookieconsent": "^2.8.9"
    }
}
